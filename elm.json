        "elm/html": "1.0.0 <= v < 1.0.1",
        "elm-community/list-extra": "8.7.0 <= v < 8.7.1",
        "elm-community/maybe-extra": "5.3.0 <= v < 5.3.1",
        "folkertdev/elm-state": "3.0.1 <= v < 3.0.2"
    },
    "test-dependencies": {
        "elm-explorations/test": "2.2.0 <= v < 2.2.1"
    }
